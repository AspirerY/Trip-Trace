page,.echarts_page,ec-canvas {
  width: 100%;
  height: 100%;
}

.container {
  height: 90%;
}

.button_wrapper {
  height: 10%;
  /* text-align: center; */
  background-color: #404a59;
  display: flex;
  justify-content: center;
  align-items: center;
}
.button_wrapper button {
  font-size: 14px;
  line-height: 1;
  padding: 10px;
  display: inline-block;
}
.button_wrapper button + button {
  margin-left: 10px;
}